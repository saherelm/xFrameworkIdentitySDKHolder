{
  "name": "x-framework-identity-sdkholder",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generateDocs": "compodoc -p projects/x-framework-identity-sdk/tsconfig.lib.json --theme material --hideGenerator --disableSourceCode --disableDomTree --disableTemplateTab --disableStyleTab --disablePrivate --disableLifeCycleHooks --disableRoutesGraph --disableCoverage --customFavicon ../xFrameworkAppCreator/resources/favicon.png --customLogo ../xFrameworkAppCreator/resources/favicon.png",
    "serveDocs": "compodoc -p projects/x-framework-identity-sdk/tsconfig.lib.json --theme material -s",
    "prepareDependencies": "npm i --save-dev ../pack/x-framework-core-0.0.1.tgz ../pack/x-framework-services-0.0.1.tgz",
    "preBuildModule": "shx true --silent rm -rf dist && shx true --silent rm ../pack/x-framework-identity-sdk*.tgz && npm run prepareDependencies",
    "buildModule": "npm run preBuildModule && ng build --prod x-framework-identity-sdk",
    "packModule": "cd dist/x-framework-identity-sdk && npm pack && shx mv *.tgz ../../../pack && cd ../..",
    "cleanModule": "shx true --silent rm -rf true && shx true --silent rm ./package-lock.json && shx true --silent rm -rf node_modules && npm i --save-dev shx@0.3.2",
    "refreshModule": "npm run cleanModule && npm i",
    "publishModule": "npm run buildModule && npm run packModule"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.14",
    "@angular/common": "~10.0.14",
    "@angular/compiler": "~10.0.14",
    "@angular/core": "~10.0.14",
    "@angular/forms": "~10.0.14",
    "@angular/platform-browser": "~10.0.14",
    "@angular/platform-browser-dynamic": "~10.0.14",
    "@angular/router": "~10.0.14",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "~10.0.8",
    "@angular/compiler-cli": "~10.0.14",
    "@angular/material": "^8.2.3",
    "@angular/material-moment-adapter": "^8.2.3",
    "@ionic/angular": "^5.1.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "crypto-js": "^3.3.0",
    "jalali-moment": "^3.3.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "libphonenumber-js": "^1.7.48",
    "md5-typescript": "^1.0.5",
    "ng-packagr": "^10.0.0",
    "ngx-device-detector": "^1.4.1",
    "protractor": "~7.0.0",
    "shx": "^0.3.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5",
    "x-framework-core": "file:../pack/x-framework-core-0.0.1.tgz",
    "x-framework-services": "file:../pack/x-framework-services-0.0.1.tgz"
  }
}
